<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="ResourceName">

    <com.healthy.library.widget.TopBar
        android:id="@+id/top_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:show_moss="true"
        app:title="憨妈赚" />

    <com.healthy.library.widget.StatusLayout
        android:id="@+id/layout_status"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/top_bar"
        tools:status_init="content">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.core.widget.NestedScrollView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                app:layout_constraintBottom_toTopOf="@+id/bottomConstraintLayout"
                app:layout_constraintTop_toTopOf="parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/han_mom_topConstraintLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/han_mom_background"
                        app:layout_constraintTop_toTopOf="parent">

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/top_constraintLayout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toTopOf="parent">

                            <ImageView
                                android:id="@+id/topImg"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:scaleType="fitXY"
                                android:src="@drawable/han_mom_top_back"
                                app:layout_constraintTop_toTopOf="parent" />

                            <androidx.cardview.widget.CardView
                                android:layout_width="match_parent"
                                android:layout_height="200dp"
                                android:layout_marginLeft="10dp"
                                android:layout_marginRight="10dp"
                                android:elevation="0dp"
                                app:cardCornerRadius="12dp"
                                app:layout_constraintLeft_toLeftOf="parent"
                                app:layout_constraintRight_toRightOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/topImg">

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <TextView
                                        android:id="@+id/addTxt"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:paddingTop="20dp"
                                        android:paddingBottom="20dp"
                                        android:text="加入合伙人一起赚钱"
                                        android:textColor="#ff996a40"
                                        android:textSize="20sp"
                                        android:textStyle="bold"
                                        app:layout_constraintLeft_toLeftOf="parent"
                                        app:layout_constraintRight_toRightOf="parent"
                                        app:layout_constraintTop_toTopOf="parent" />

                                    <androidx.constraintlayout.widget.ConstraintLayout
                                        android:id="@+id/icon_grid"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        app:layout_constraintLeft_toLeftOf="parent"
                                        app:layout_constraintRight_toRightOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/addTxt">

                                        <ImageView
                                            android:id="@+id/han_mom_share_money"
                                            android:layout_width="44dp"
                                            android:layout_height="44dp"
                                            android:src="@drawable/han_mom_share_money"
                                            app:layout_constraintLeft_toLeftOf="parent"
                                            app:layout_constraintTop_toTopOf="parent" />

                                        <ImageView
                                            android:id="@+id/han_mom_save_money"
                                            android:layout_width="44dp"
                                            android:layout_height="44dp"
                                            android:layout_marginLeft="65dp"
                                            android:src="@drawable/han_mom_save_money"
                                            app:layout_constraintLeft_toRightOf="@+id/han_mom_share_money"
                                            app:layout_constraintTop_toTopOf="parent" />

                                        <ImageView
                                            android:id="@+id/han_mom_immediate_money"
                                            android:layout_width="44dp"
                                            android:layout_height="44dp"
                                            android:layout_marginLeft="65dp"
                                            android:src="@drawable/han_mom_immediate_money"
                                            app:layout_constraintLeft_toRightOf="@+id/han_mom_save_money"
                                            app:layout_constraintTop_toTopOf="parent" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:paddingTop="6dp"
                                            android:text="分享赚钱"
                                            android:textColor="#ff444444"
                                            android:textSize="12sp"
                                            app:layout_constraintLeft_toLeftOf="@+id/han_mom_share_money"
                                            app:layout_constraintRight_toRightOf="@+id/han_mom_share_money"
                                            app:layout_constraintTop_toBottomOf="@+id/han_mom_share_money" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:paddingTop="6dp"
                                            android:text="自购省钱"
                                            android:textColor="#ff444444"
                                            android:textSize="12sp"
                                            app:layout_constraintLeft_toLeftOf="@+id/han_mom_save_money"
                                            app:layout_constraintRight_toRightOf="@+id/han_mom_save_money"
                                            app:layout_constraintTop_toBottomOf="@+id/han_mom_save_money" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:paddingTop="6dp"
                                            android:text="立即到账"
                                            android:textColor="#ff444444"
                                            android:textSize="12sp"
                                            app:layout_constraintLeft_toLeftOf="@+id/han_mom_immediate_money"
                                            app:layout_constraintRight_toRightOf="@+id/han_mom_immediate_money"
                                            app:layout_constraintTop_toBottomOf="@+id/han_mom_immediate_money" />
                                    </androidx.constraintlayout.widget.ConstraintLayout>

                                    <TextView
                                        android:id="@+id/goWx"
                                        android:layout_width="match_parent"
                                        android:layout_height="42dp"
                                        android:layout_marginLeft="30dp"
                                        android:layout_marginTop="15dp"
                                        android:layout_marginRight="30dp"
                                        android:background="@drawable/shape_hanmom_btn"
                                        android:gravity="center"
                                        android:text="立即加入"
                                        android:textColor="#fffdfdfd"
                                        android:textSize="17sp"
                                        app:layout_constraintTop_toBottomOf="@+id/icon_grid" />

                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </androidx.cardview.widget.CardView>
                        </androidx.constraintlayout.widget.ConstraintLayout>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/people_card"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:elevation="0dp"
                            app:cardCornerRadius="12dp"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/top_constraintLayout">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:id="@+id/han_momTopLiner"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="20dp"
                                    android:layout_marginRight="20dp"
                                    android:orientation="vertical">

                                    <com.healthy.library.widget.ImageTextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="20dp"
                                        android:drawablePadding="5dp"
                                        android:gravity="center"
                                        android:text="明星合伙人"
                                        android:textColor="#ff444444"
                                        android:textSize="15sp"
                                        android:textStyle="bold"
                                        app:drawable="@drawable/han_mom_crown"
                                        app:drawableHeight="32dp"
                                        app:drawableWidth="32dp" />

                                    <LinearLayout
                                        android:id="@+id/topLinearLayout"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="10dp"
                                        android:background="@color/white"
                                        android:orientation="vertical"
                                        android:paddingBottom="10dp">

                                        <androidx.constraintlayout.widget.ConstraintLayout
                                            android:id="@+id/top"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_margin="5dp"
                                            android:visibility="visible"
                                            app:layout_constraintLeft_toLeftOf="parent"
                                            app:layout_constraintRight_toRightOf="parent"
                                            app:layout_constraintTop_toTopOf="parent">

                                            <com.healthy.library.widget.CornerImageView
                                                android:id="@+id/userImg"
                                                android:layout_width="40dp"
                                                android:layout_height="40dp"
                                                android:src="@drawable/img_avatar_default"
                                                app:is_circle="true"
                                                app:layout_constraintLeft_toLeftOf="parent"
                                                app:layout_constraintTop_toTopOf="parent" />

                                            <TextView
                                                android:id="@+id/userNickName"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginLeft="7dp"
                                                android:layout_marginRight="12dp"
                                                android:ellipsize="end"
                                                android:maxLines="1"
                                                android:textColor="#ff444444"
                                                android:textSize="15sp"
                                                android:textStyle="bold"
                                                app:layout_constraintLeft_toRightOf="@id/userImg"
                                                app:layout_constraintRight_toLeftOf="@id/userLabel"
                                                app:layout_constraintTop_toTopOf="@id/userImg"
                                                tools:text="番茄果果" />

                                            <TextView
                                                android:id="@+id/userLabel"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginLeft="10dp"
                                                android:textColor="#ff444444"
                                                android:textSize="12sp"
                                                app:layout_constraintBottom_toBottomOf="@id/userNickName"
                                                app:layout_constraintLeft_toRightOf="@+id/userNickName"
                                                tools:text="2岁男宝的宝妈" />

                                            <TextView
                                                android:id="@+id/userContent"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="5dp"
                                                android:textColor="#ff868799"
                                                android:textSize="13sp"
                                                app:layout_constraintLeft_toLeftOf="@id/userNickName"
                                                app:layout_constraintTop_toBottomOf="@id/userNickName"
                                                tools:text="加入憨妈赚合伙人21天，收入" />

                                            <TextView
                                                android:id="@+id/userContentPrice"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="5dp"
                                                android:textColor="#FF1641"
                                                android:textSize="13sp"
                                                app:layout_constraintLeft_toRightOf="@id/userContent"
                                                app:layout_constraintTop_toBottomOf="@id/userNickName"
                                                tools:text="6898.7" />

                                            <TextView
                                                android:id="@+id/userContentPriceEnd"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="5dp"
                                                android:textColor="#ff868799"
                                                android:textSize="13sp"
                                                app:layout_constraintLeft_toRightOf="@id/userContentPrice"
                                                app:layout_constraintTop_toBottomOf="@id/userNickName"
                                                tools:text="元" />

                                        </androidx.constraintlayout.widget.ConstraintLayout>

                                        <androidx.constraintlayout.widget.ConstraintLayout
                                            android:id="@+id/top2"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_margin="5dp"
                                            android:visibility="visible"
                                            app:layout_constraintLeft_toLeftOf="parent"
                                            app:layout_constraintRight_toRightOf="parent"
                                            app:layout_constraintTop_toTopOf="parent">

                                            <com.healthy.library.widget.CornerImageView
                                                android:id="@+id/userImg2"
                                                android:layout_width="40dp"
                                                android:layout_height="40dp"
                                                android:src="@drawable/img_avatar_default"
                                                app:is_circle="true"
                                                app:layout_constraintLeft_toLeftOf="parent"
                                                app:layout_constraintTop_toTopOf="parent" />

                                            <TextView
                                                android:id="@+id/userNickName2"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginLeft="7dp"
                                                android:layout_marginRight="12dp"
                                                android:ellipsize="end"
                                                android:maxLines="1"
                                                android:textColor="#ff444444"
                                                android:textSize="15sp"
                                                android:textStyle="bold"
                                                app:layout_constraintLeft_toRightOf="@id/userImg2"
                                                app:layout_constraintRight_toLeftOf="@id/userLabel2"
                                                app:layout_constraintTop_toTopOf="@id/userImg2"
                                                tools:text="番茄果果" />

                                            <TextView
                                                android:id="@+id/userLabel2"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginLeft="10dp"
                                                android:textColor="#ff444444"
                                                android:textSize="12sp"
                                                app:layout_constraintBottom_toBottomOf="@id/userNickName2"
                                                app:layout_constraintLeft_toRightOf="@+id/userNickName2"
                                                tools:text="2岁男宝的宝妈" />

                                            <TextView
                                                android:id="@+id/userContent2"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="5dp"
                                                android:textColor="#ff868799"
                                                android:textSize="13sp"
                                                app:layout_constraintLeft_toLeftOf="@id/userNickName2"
                                                app:layout_constraintTop_toBottomOf="@id/userNickName2"
                                                tools:text="加入憨妈赚合伙人21天，收入" />

                                            <TextView
                                                android:id="@+id/userContentPrice2"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="5dp"
                                                android:textColor="#FF1641"
                                                android:textSize="13sp"
                                                app:layout_constraintLeft_toRightOf="@id/userContent2"
                                                app:layout_constraintTop_toBottomOf="@id/userNickName2"
                                                tools:text="6898.7" />

                                            <TextView
                                                android:id="@+id/userContentPriceEnd2"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginTop="5dp"
                                                android:textColor="#ff868799"
                                                android:textSize="13sp"
                                                app:layout_constraintLeft_toRightOf="@id/userContentPrice2"
                                                app:layout_constraintTop_toBottomOf="@id/userNickName2"
                                                tools:text="元" />

                                        </androidx.constraintlayout.widget.ConstraintLayout>
                                    </LinearLayout>
                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/bottomLinearLayout"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="#fffffae4"
                                    android:orientation="vertical">

                                    <com.healthy.library.widget.AutoPollRecyclerView
                                        android:id="@+id/autoPollRecyclerView"
                                        android:layout_width="match_parent"
                                        android:layout_height="80dp"
                                        android:layout_marginLeft="20dp"
                                        android:layout_marginRight="20dp" />

                                    <View
                                        android:layout_width="match_parent"
                                        android:layout_height="15dp" />
                                </LinearLayout>
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/addCard"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="10dp"
                            android:elevation="0dp"
                            app:cardCornerRadius="12dp"
                            app:layout_constraintTop_toBottomOf="@+id/people_card">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:id="@+id/rightsLayout"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="22dp"
                                    android:layout_marginRight="22dp"
                                    app:layout_constraintTop_toTopOf="parent">

                                    <TextView
                                        android:id="@+id/title"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="30dp"
                                        android:text="专属权益 开通即享"
                                        android:textColor="#ff444444"
                                        android:textSize="20sp"
                                        android:textStyle="bold"
                                        app:layout_constraintLeft_toLeftOf="parent"
                                        app:layout_constraintRight_toRightOf="parent"
                                        app:layout_constraintTop_toTopOf="parent" />

                                    <LinearLayout
                                        android:id="@+id/rightsLiner"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="10dp"
                                        android:orientation="vertical"
                                        app:layout_constraintLeft_toLeftOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/title">

<!--                                        <include layout="@layout/item_hanmom_rights_layout"></include>-->
                                    </LinearLayout>

                                    <View
                                        android:id="@+id/hightView"
                                        android:layout_width="match_parent"
                                        android:layout_height="20dp"
                                        app:layout_constraintTop_toBottomOf="@+id/rightsLiner" />
                                </androidx.constraintlayout.widget.ConstraintLayout>

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="#fff9f9f9"
                                    android:paddingLeft="22dp"
                                    android:paddingRight="22dp">

                                    <androidx.constraintlayout.widget.ConstraintLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="10dp"
                                        android:layout_marginBottom="10dp"
                                        app:layout_constraintBottom_toBottomOf="parent"
                                        app:layout_constraintLeft_toLeftOf="parent"
                                        app:layout_constraintRight_toRightOf="parent"
                                        app:layout_constraintTop_toTopOf="parent">

                                        <TextView
                                            android:id="@+id/hanMom_hehuoren"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="憨妈赚合伙人"
                                            android:textColor="#ffab8749"
                                            android:textSize="18sp"
                                            app:layout_constraintLeft_toLeftOf="parent"
                                            app:layout_constraintTop_toTopOf="parent" />

                                        <TextView
                                            android:id="@+id/goodsPrice"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="10dp"
                                            android:textColor="#ff666666"
                                            android:textSize="15sp"
                                            app:layout_constraintLeft_toLeftOf="parent"
                                            app:layout_constraintTop_toBottomOf="@+id/hanMom_hehuoren"
                                            tools:text="￥30" />

                                        <TextView
                                            android:id="@+id/goodsDays"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:paddingLeft="6dp"
                                            android:textColor="#ff666666"
                                            android:textSize="12sp"
                                            app:layout_constraintBottom_toBottomOf="@+id/goodsPrice"
                                            app:layout_constraintLeft_toRightOf="@+id/goodsPrice"
                                            tools:text="365天" />

                                        <com.healthy.library.widget.ImageTextView
                                            android:id="@+id/localTxt"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:textColor="#ff666666"
                                            android:textSize="13sp"
                                            app:drawable="@mipmap/servicecenter_nearby_address"
                                            app:drawableHeight="16dp"
                                            app:drawableWidth="16dp"
                                            app:layout_constraintBottom_toTopOf="@+id/addBtn"
                                            app:layout_constraintLeft_toLeftOf="@+id/addBtn"
                                            app:layout_constraintRight_toRightOf="@+id/addBtn"
                                            tools:text="苏州市" />

                                        <TextView
                                            android:id="@+id/addBtn"
                                            android:layout_width="84dp"
                                            android:layout_height="32dp"
                                            android:layout_marginTop="6dp"
                                            android:background="@drawable/shape_hanmom_btn_backshape"
                                            android:gravity="center"
                                            android:text="立即加入"
                                            android:textColor="#ffffffff"
                                            android:textSize="14sp"
                                            app:layout_constraintRight_toRightOf="parent"
                                            app:layout_constraintTop_toBottomOf="@+id/localTxt" />
                                    </androidx.constraintlayout.widget.ConstraintLayout>
                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </LinearLayout>

                        </androidx.cardview.widget.CardView>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="10dp"
                            app:layout_constraintTop_toBottomOf="@+id/addCard" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <LinearLayout
                        android:id="@+id/han_mom_bottomConstraintLayout"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="vertical">


                        <com.flyco.tablayout.SlidingTabLayout
                            android:id="@+id/slidingTabLayout"
                            android:layout_width="match_parent"
                            android:layout_height="44dp"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            app:tl_indicator_color="#FF6177"
                            app:tl_indicator_corner_radius="1.5dp"
                            app:tl_indicator_height="4dp"
                            app:tl_indicator_margin_top="10dp"
                            app:tl_indicator_width="30dp"
                            app:tl_tab_padding="10dp"
                            app:tl_tab_space_equal="true"
                            app:tl_textBold="SELECT"
                            app:tl_textSelectColor="#222222"
                            app:tl_textUnselectColor="#9596A4"
                            app:tl_textsize="14sp" />

                        <com.health.mine.widget.CustomViewPager
                            android:id="@+id/viewPager"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:background="#FFF" />
                    </LinearLayout>
                </LinearLayout>
            </androidx.core.widget.NestedScrollView>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/bottomConstraintLayout"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="13dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <com.healthy.library.widget.ImageTextView
                        android:id="@+id/bottomLocalTxt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="本地暂未入驻"
                        android:textColor="#ff666666"
                        android:textSize="13sp"
                        app:drawable="@mipmap/servicecenter_nearby_address"
                        app:drawableHeight="16dp"
                        app:drawableWidth="16dp"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="憨妈赚合伙人"
                        android:textColor="#ffab8749"
                        android:textSize="18sp"
                        app:layout_constraintLeft_toLeftOf="@+id/bottomLocalTxt"
                        app:layout_constraintTop_toBottomOf="@+id/bottomLocalTxt" />
                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/bottom_toPay"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/han_mom_bottom_back"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toRightOf="parent">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <TextView
                            android:id="@+id/symbolTxt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textColor="#ffffffff"
                            android:textSize="18sp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintLeft_toLeftOf="parent" />

                        <TextView
                            android:id="@+id/priceTxt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:includeFontPadding="false"
                            android:paddingLeft="20dp"
                            android:textColor="#ffffffff"
                            android:textSize="40sp"
                            app:layout_constraintBottom_toBottomOf="@+id/symbolTxt"
                            app:layout_constraintLeft_toRightOf="@+id/symbolTxt"
                            tools:text="30" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            app:layout_constraintBottom_toBottomOf="@+id/priceTxt"
                            app:layout_constraintLeft_toRightOf="@+id/priceTxt"
                            app:layout_constraintTop_toTopOf="@+id/priceTxt">

                            <TextView
                                android:id="@+id/bottomDays"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:textColor="#ffffffff"
                                android:textSize="12sp"
                                tools:text="365天" />

                            <TextView
                                android:id="@+id/submitTxt"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:text="立即加入"
                                android:textColor="#ffffffff"
                                android:textSize="14sp" />
                        </LinearLayout>


                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </com.healthy.library.widget.StatusLayout>


</androidx.constraintlayout.widget.ConstraintLayout>