<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activity.DetailedActivity"
    tools:ignore="MissingConstraints,MissingDefaultResource">
    <com.healthy.library.widget.TopBar
        android:id="@+id/top_bar"
        android:layout_marginTop="@dimen/status_2020"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:elevation="0dp"
        app:back_img_src="@drawable/ic_back"
        app:bg_color="#00000000"
        app:submit_img_src="@drawable/ic_seach_sound"
        app:is_show_submit_image="false"
        app:layout_constraintTop_toTopOf="parent"
        app:show_moss="false"
        app:title_color="#000"
        app:title="宝宝爱听" />
    <FrameLayout
        android:id="@+id/childFrame"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></FrameLayout>

</LinearLayout>