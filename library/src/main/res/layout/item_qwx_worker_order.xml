<?xml version="1.0" encoding="utf-8"?>
<com.example.lib_ShapeView.layout.ShapeConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_marginTop="10dp"
    android:layout_marginHorizontal="10dp"
    android:layout_height="wrap_content"
    tools:ignore="MissingDefaultResource">
    <ImageView
        android:id="@+id/workBg"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintDimensionRatio="710:136"
        android:src="@drawable/qwx_bg_order"/>
    <com.healthy.library.widget.CornerImageView
        android:layout_width="48dp"
        android:layout_height="48dp"
        app:is_circle="true"
        android:id="@+id/workImg"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="12dp"
        app:layout_constraintTop_toTopOf="@id/workBg"
        app:layout_constraintLeft_toLeftOf="@id/workBg"
        android:src="@drawable/ic_sex_women"
        />
    <ImageView
        android:layout_width="14dp"
        android:layout_height="13dp"
        app:layout_constraintRight_toRightOf="@id/workImg"
        app:layout_constraintBottom_toBottomOf="@id/workImg"
        android:src="@drawable/ww_ic_launcher_alpha"/>
    <TextView
        android:id="@+id/workName"
        app:layout_constraintLeft_toRightOf="@id/workImg"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="12dp"
        android:layout_marginLeft="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="沪小二"
        android:textColor="#ff222222"
        android:textSize="16sp"
        />
    <TextView
        android:id="@+id/workButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@id/workName"
        app:layout_constraintTop_toBottomOf="@id/workName"
        android:layout_marginTop="6dp"
        android:text="点击可快速联系您的母婴顾问"
        android:textColor="#ffff94a5"
        android:textSize="12sp"
        />
</com.example.lib_ShapeView.layout.ShapeConstraintLayout>