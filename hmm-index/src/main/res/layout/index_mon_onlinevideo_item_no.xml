<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:id="@+id/onlinevideoLL"
        android:layout_height="wrap_content"
        tools:ignore="MissingDefaultResource">

        <com.healthy.library.widget.CornerImageView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:id="@+id/videoFlash"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:src="@drawable/faq_top_scroll"
            app:corner_radius="6dp"
            app:layout_constraintDimensionRatio="130:74"
            app:layout_constraintBottom_toBottomOf="parent">
        </com.healthy.library.widget.CornerImageView>
        <ImageView
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintWidth_percent=".14"
            android:id="@+id/videoGo"
            app:layout_constraintHeight_percent=".26"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:src="@drawable/ic_video_playwww">

        </ImageView>

    </androidx.constraintlayout.widget.ConstraintLayout>
</LinearLayout>


