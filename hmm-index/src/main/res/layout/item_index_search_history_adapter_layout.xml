<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/title_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingLeft="14dp"
    android:paddingTop="10dp"
    android:paddingRight="14dp"
    android:paddingBottom="10dp"
    tools:ignore="MissingConstraints,MissingDefaultResource,ResourceName">

    <TextView
        android:id="@+id/tv_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="搜索历史"
        android:textColor="@color/color_333333"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/iv_delete_icon"
        android:layout_width="26dp"
        android:layout_height="26dp"
        android:padding="4dp"
        android:src="@drawable/ic_city_delete"
        app:layout_constraintBottom_toBottomOf="@+id/tv_title"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tv_title" />

    <com.healthy.library.widget.StackLabel
        android:id="@+id/stackLabelView"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        app:deleteButton="false"
        app:isBold="false"
        app:itemMargin="4dp"
        app:labelBackground="@drawable/shape_index_search_lable_bg"
        app:layout_constraintTop_toBottomOf="@+id/tv_title"
        app:paddingHorizontal="12dp"
        app:paddingVertical="4dp"
        app:textColor="#333333"
        app:textSize="12sp" />

</androidx.constraintlayout.widget.ConstraintLayout>