<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/parent_category"
    android:layout_margin="4dp"
    android:background="@drawable/shape_item_faq"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:paddingBottom="5dp"
        android:layout_marginBottom="6dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/goodsImg_goods4"
            android:layout_width="match_parent"
            android:layout_height="175dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:scaleType="centerCrop"
            android:src="@drawable/img_1_1_default2">
        </ImageView>
        <TextView
            android:id="@+id/goodsTitle_goods4"
            app:layout_constraintTop_toBottomOf="@id/goodsImg_goods4"
            android:layout_width="0dp"
            android:layout_marginTop="6dp"
            android:layout_marginLeft="6dp"
            android:layout_marginRight="6dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_height="wrap_content"
            android:maxLines="2"
            android:ellipsize="end"
            android:text="美素佳儿婴幼儿配方奶粉1段800g"
            android:textColor="#ff222222"
            android:textSize="14sp"
            />
        <TextView
            android:id="@+id/goodSpace_goods4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="粉色 100cm"
            android:visibility="gone"
            android:layout_marginTop="4dp"
            android:textColor="#ff868799"
            android:textSize="11sp"
            app:layout_constraintLeft_toLeftOf="@+id/goodsTitle_goods4"
            app:layout_constraintTop_toBottomOf="@+id/goodsTitle_goods4" />
        <TextView
            android:textStyle="bold"
            android:id="@+id/mall_goods_moneyflag_goods4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="¥"
            android:layout_marginBottom="2dp"
            android:textColor="#FA3C5A"
            android:textSize="13sp"
            app:layout_constraintLeft_toLeftOf="@id/goodsTitle_goods4"
            app:layout_constraintBottom_toBottomOf="@id/mall_goods_moneyvalue_goods4"/>

        <TextView
            android:id="@+id/mall_goods_moneyvalue_goods4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="2dp"
            android:text="0"
            android:layout_marginTop="4dp"
            android:textStyle="bold"
            android:textColor="#FA3C5A"
            android:textSize="18sp"
            app:layout_constraintTop_toBottomOf="@id/goodSpace_goods4"
            app:layout_constraintLeft_toRightOf="@id/mall_goods_moneyflag_goods4" />
        <TextView
            android:id="@+id/mall_goods_moneyvalue_org_goods4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="9dp"
            app:layout_constraintTop_toTopOf="@id/mall_goods_moneyvalue_goods4"
            app:layout_constraintBottom_toBottomOf="@id/mall_goods_moneyvalue_goods4"
            app:layout_constraintLeft_toRightOf="@+id/mall_goods_moneyvalue_goods4"
            tools:text="¥0"
            android:textColor="#ff868799"
            android:textSize="12sp"
            />
        <TextView
            android:id="@+id/tehui_goods4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:includeFontPadding="false"
            android:background="@drawable/act_red_bg"
            app:layout_constraintTop_toTopOf="@id/mall_goods_moneyvalue_goods4"
            app:layout_constraintBottom_toBottomOf="@id/mall_goods_moneyvalue_goods4"
            app:layout_constraintLeft_toRightOf="@+id/mall_goods_moneyvalue_goods4"
            android:text="特惠价"
            android:visibility="gone"
            android:textColor="#ffffffff"
            android:textSize="10sp"
            />
        <com.healthy.library.widget.AutoClickImageView
            android:id="@+id/passbasket_goods4"
            android:layout_width="27dp"
            android:layout_height="27dp"
            android:layout_marginTop="5dp"
            android:layout_marginRight="8dp"
            android:layout_marginBottom="2dp"
            android:src="@drawable/basket_red"
            app:layout_constraintBottom_toBottomOf="@id/mall_goods_moneyvalue_goods4"
            app:layout_constraintRight_toRightOf="parent" />
        <View
            android:layout_width="match_parent"
            android:layout_height="5dp"
            app:layout_constraintTop_toBottomOf="@+id/passbasket_goods4" />
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="16dp"
            android:layout_marginTop="4dp"
            android:id="@+id/vipGoldP"
            android:orientation="horizontal"
            android:visibility="gone"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/mall_goods_moneyvalue_goods4">
            <ImageView
                android:layout_width="20dp"
                android:layout_height="16dp"
                android:src="@drawable/vip_gold_left"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:paddingLeft="3dp"
                android:paddingRight="3dp"
                android:background="@drawable/vip_gold_right"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="黑金卡"
                    android:gravity="center"
                    android:includeFontPadding="false"
                    android:textColor="#ff303037"
                    android:textSize="10sp"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="¥379"
                    android:gravity="center"
                    android:includeFontPadding="false"
                    android:textColor="#ff303037"
                    android:textSize="11sp"
                    />
            </LinearLayout>
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
